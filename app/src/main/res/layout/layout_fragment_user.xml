<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:imagecontrol="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/default_backgroud_color"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/image_title_background"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:gravity="top"
                    android:orientation="horizontal"

                    >

                    <com.example.wsq.android.view.RoundImageView
                        android:id="@+id/roundImage_header"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginLeft="@dimen/defalut_layout_height"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/image_header_bg"
                        imagecontrol:border_inside_color="#FFFFFF"
                        imagecontrol:border_outside_color="#E4E4E4"
                        imagecontrol:border_thickness="2dp" />
                    />

                    <LinearLayout
                        android:id="@+id/ll_user_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="top"
                        android:orientation="horizontal">


                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/default_margin"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tv_username"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/color_white"
                                android:textSize="@dimen/default_text_title_size" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/default_radius"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_sex"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/color_white"
                                    android:textSize="@dimen/default_text_size" />

                                <View
                                    android:layout_width="2dp"
                                    android:layout_height="match_parent"
                                    android:layout_margin="@dimen/default_radius"
                                    android:background="@color/color_white" />

                                <TextView
                                    android:id="@+id/tv_role"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/color_white"
                                    android:textSize="@dimen/default_text_size" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/ll_sign_integral"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/default_radius"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:visibility="gone"
                                >

                                <LinearLayout
                                    android:id="@+id/ll_iv_integral"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:layout_margin="@dimen/default_radius"
                                    >

                                    <ImageView
                                        android:id="@+id/iv_integral"
                                        android:layout_width="15dp"
                                        android:layout_height="15dp"
                                        android:src="@drawable/image_integral" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/default_radius"
                                        android:text="积分 10分"
                                        android:textColor="@color/color_white"
                                        android:textSize="@dimen/default_text_size" />
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/tv_sign"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/default_margin"
                                    android:background="@drawable/shape_sign_button"
                                    android:paddingBottom="2dp"
                                    android:paddingLeft="@dimen/radius_popup"
                                    android:paddingRight="@dimen/radius_popup"
                                    android:paddingTop="2dp"
                                    android:text="签到"
                                    android:textColor="@color/defalut_title_color"
                                    android:textSize="@dimen/default_text_size" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="@dimen/default_margin"
                                android:src="@drawable/image_left" />
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="@drawable/shape_user_c" />
            </LinearLayout>


            <!--企业工程师 企业管理工程师 任务部分-->
            <LinearLayout
                android:id="@+id/ll_company_order"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@color/color_white">

                <RelativeLayout
                    android:id="@+id/ll_order_unfinish"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_tab_untreated" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="未审核" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_uncheck"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/ll_order_finish"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_my_approved" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="已审核" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_hascheck"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/ll_order_handler"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_clz" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="处理中" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_processed"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/ll_order"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_tab_finish" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="已完成" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_done"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>
            </LinearLayout>

            <!--服务工程师 任务部分-->
            <LinearLayout
                android:id="@+id/ll_server_order"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:background="@color/color_white">

                <RelativeLayout
                    android:id="@+id/ll_order_fllocation"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_my_approved" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="已分配" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_assigned"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/ll_order_progress"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/image_clz" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="处理中" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_server_processed"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/ll_order_feedback"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_tab_documents" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="反馈中" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_feedback"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/ll_server_finish"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/default_image_size"
                            android:layout_height="@dimen/default_image_size"
                            android:src="@drawable/image_tab_finish" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/default_radius"
                            android:text="完成" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_server_done"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="@dimen/radius_popup"
                        android:background="@drawable/shape_num"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/color_white"
                        android:visibility="gone" />
                </RelativeLayout>
            </LinearLayout>


            <!--账户余额部分-->

            <LinearLayout
                android:id="@+id/ll_fee"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="@dimen/radius_popup"
                android:background="@color/color_white">

                <LinearLayout
                    android:id="@+id/ll_balance"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_money"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0.00"
                        android:textSize="@dimen/default_text_max_size"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="余额" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_pay_Record"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_pay_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textSize="@dimen/default_text_max_size"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="账单" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_money_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0.00"
                        android:textSize="@dimen/default_text_max_size"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="收入" />
                </LinearLayout>

            </LinearLayout>


            <!--服务工程师-->
            <LinearLayout
                android:id="@+id/ll_device_server"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="@dimen/radius_popup"
                android:background="@color/color_white"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/ll_device_report"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_report" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="写报告" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_device_bank_code"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_bank" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="银行卡" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_device_server_share"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_share" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="分享" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_device_knowledge"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_knowledge" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="圈内知识" />

                </LinearLayout>
            </LinearLayout>
            <!--设备部分-->
            <LinearLayout
                android:id="@+id/ll_manager"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="@dimen/radius_popup"
                android:background="@color/color_white">

                <LinearLayout
                    android:id="@+id/ll_device_assert"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_sbwh" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="我要报修" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_fault"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_knowledge" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="资料" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_manager_shared"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_share" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="分享" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_manager_upload"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_tab_repair" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="设备上传" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_marginTop="2dp"
                android:background="@color/color_white">

                <LinearLayout
                    android:id="@+id/ll_collect"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_my_sc" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="我的收藏" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_server_call"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/image_kf" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="客服中心" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_password"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/chang_password" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="修改密码" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_about"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/default_image_size"
                        android:layout_height="@dimen/default_image_size"
                        android:src="@drawable/tab_about" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/default_radius"
                        android:text="关于我们" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/tv_quit"
                style="@style/default_button"
                android:text="退出登录"

                />

        </LinearLayout>
    </ScrollView>
</LinearLayout>
